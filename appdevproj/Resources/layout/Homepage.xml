<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#181A20">

   <ImageButton
        android:id="@+id/homeBTN"
       android:layout_width="70dp"
       android:layout_height="70dp"
       android:scaleType="fitCenter"
       android:src="@drawable/hme"
       android:backgroundTint="#181A20"
       android:layout_marginTop="820dp"
       android:layout_marginLeft="10dp"/>

    <ImageButton
        android:id="@+id/movieBTN"
       android:layout_width="70dp"
       android:layout_height="70dp"
       android:scaleType="fitCenter"
       android:src="@drawable/movie"
       android:backgroundTint="#181A20"
       android:layout_marginTop="820dp"
       android:layout_marginLeft="90dp"/>
    <ImageButton
        android:id="@+id/ticketBTN"
       android:layout_width="70dp"
       android:layout_height="70dp"
       android:scaleType="fitCenter"
       android:src="@drawable/tickets"
       android:backgroundTint="#181A20"
       android:layout_marginTop="820dp"
        android:layout_marginLeft="170dp"/>
    <ImageButton
        android:id="@+id/srchBTN"
       android:layout_width="70dp"
       android:layout_height="70dp"
       android:scaleType="fitCenter"
       android:src="@drawable/searchh"
       android:backgroundTint="#181A20"
       android:layout_marginTop="820dp"
       android:layout_marginLeft="250dp"/>
    <ImageButton
        android:id="@+id/accBTN"
       android:layout_width="70dp"
       android:layout_height="70dp"
       android:scaleType="fitCenter"
       android:src="@drawable/acc"
       android:backgroundTint="#181A20"
       android:layout_marginTop="820dp"
        android:layout_marginLeft="330dp"/>
</RelativeLayout>
